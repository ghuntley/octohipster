<Application x:Class="OctoHipster.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:octoHipster="clr-namespace:OctoHipster">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <octoHipster:AppBootstrapper x:Key="bootstrapper" />
                </ResourceDictionary>
            <ResourceDictionary Source="Resources/Colours.xaml"/>
            <ResourceDictionary Source="Resources/SpinKit.Styles.xaml"/>
        </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
