<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Background">#FFFFFF</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>

    <Color x:Key="SecondaryBackground">#AEAEAE</Color>
    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="{StaticResource SecondaryBackground}"/>

    <Color x:Key="AccentColor">#E64C66</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>

    <Color x:Key="AccentColor2">#2D3E50</Color>
    <SolidColorBrush x:Key="AccentBrush2" Color="{StaticResource AccentColor2}"/>

    <Color x:Key="AccentColor3">#1BBC9B</Color>
    <SolidColorBrush x:Key="AccentBrush3" Color="{StaticResource AccentColor3}"/>
</ResourceDictionary>